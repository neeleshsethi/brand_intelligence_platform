# Docker Compose Environment Variables
# Copy this to .env and customize for your setup

# Demo Modes (defaults to demo mode for easy testing)
DEMO_MODE=true
MOCK_MODE=true

# OpenAI (only needed if MOCK_MODE=false)
OPENAI_API_KEY=

# LangSmith Tracing (optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=pfizer-brand-planning

# Supabase (only needed if MOCK_MODE=false)
SUPABASE_URL=
SUPABASE_KEY=
